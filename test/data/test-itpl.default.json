{
    "key": "#!jst:Hello ${name}, welcome to ${place}!",
    "key2": {
        "array": [
            "#!jst:value1: ${value1}",
            "#!jst:value2: ${value2}",
            "#!jst:sum: ${value1 + value2}"
        ],
        "object": {
            "non": "nothing",
            "itpl": "#!jst:${(new Date()).toString()}"
        },
        "jsv1": "#!jsv:100+200*0.5",
